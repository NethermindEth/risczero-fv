open MLIRNotation

-- The state obtained by running Code.part20 on st
def part20_state (st: State) : State :=
  
          ((((st[felts][{ name := "%33" }] ←
                  Option.get! (State.felts st { name := "%32" }) +
                    Option.get! (State.felts st { name := "%27" }))["%25"] ←ₛ
                getImpl st { name := "data" } 0 0)[felts][{ name := "%34" }] ←
              Option.get! (State.felts st { name := "%32" }) + Option.get! (State.felts st { name := "%27" }) +
                Option.get!
                  (State.felts
                    ((st[felts][{ name := "%33" }] ←
                        Option.get! (State.felts st { name := "%32" }) +
                          Option.get! (State.felts st { name := "%27" }))["%25"] ←ₛ
                      getImpl st { name := "data" } 0 0)
                    { name := "%25" }))["%35"] ←ₛ
            getImpl
              (st[felts][{ name := "%33" }] ←
                Option.get! (State.felts st { name := "%32" }) + Option.get! (State.felts st { name := "%27" }))
              { name := "data" } 0 10) 

def part20_drops (st: State) : State :=
  State.dropFelts (State.dropFelts (State.dropFelts (State.dropFelts (st) ⟨"%27"⟩) ⟨"%32"⟩) ⟨"%33"⟩) ⟨"%25"⟩

-- Run the program from part20 onwards by using part20_state rather than Code.part20
